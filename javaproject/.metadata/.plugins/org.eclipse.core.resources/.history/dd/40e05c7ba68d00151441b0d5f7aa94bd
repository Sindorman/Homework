package Homework7;

public class NumberTwo {
	public static void main(String[] args){
		int[] array = {1, -2, 4, -4, 9, -6, 16, -8, 25, -10};
		//stdev(array);		
	}
	public static void stdev(int[] array){
		int AverageNumber = Average(array);
		System.out.print(AverageNumber);
		
	}
	public static int Average(int[] Average){
		int sum = 0;
		for(int x = 0; x < Average.length; x++){
			
			sum =+ Math.abs(Average[x]); 
		}
		
		return sum/Average.length;
	}
}
