
public class Test {

	public static void main(String[] args) {
		//TimeSpan test = new TimeSpan(2, 2);
		//test.scale(99999999);
		//System.out.print(test.toString());
		double minutes = 0;
		System.out.println(minutes);
		minutes = 122;
		System.out.println(minutes);
		minutes *= 99999999;
		System.out.println(minutes);
		int test = (int) minutes;
		System.out.println(test);
		int hours = 0;
		int RealMinutes = 0;
		while(test >= 60){
			hours ++;
			RealMinutes -=60;
		}
		System.out.println(hours + "h" + RealMinutes + "m");
	}

}
