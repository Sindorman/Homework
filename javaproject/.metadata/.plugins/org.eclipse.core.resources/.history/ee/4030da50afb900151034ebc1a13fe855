
public class ShapeTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Shape [] test = new Shape[9];
		test [0] = new Rectangle (13, 23);
		test [1] = new Rectangle (32, 34);
		test [2] = new Rectangle (13, 23);
		test [3] = new Circle (4);
		test [4] = new Circle (6);
		test [5] = new Circle (4);
		test [6] = new Triangle (13, 23, 45);
		test [7] = new Triangle (13, 23, 78);
		test [8] = new Triangle (13, 23, 45);
		System.out.println(test[1] + "=" + test[1] + test[0].equals(test[1]));
	}

}
