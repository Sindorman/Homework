package Homework7;

public class NumberTwo {
	public static void main(String[] args){
		int[] array = {1, -2, 4, -4, 9, -6, 16, -8, 25, -10};
		stdev(array);
		System.out.print( " " + Summation( Average(array),array));
	}
	public static void stdev(int[] array){
		double AverageNumber = Average(array);
		System.out.print(AverageNumber);
		
	}
	public static double Average(int[] AverageNum){
		double sum = 0;
		for(int x = 0; x < AverageNum.length; x++){
			
			sum += Math.abs(AverageNum[x]); 
		}
		
		return sum/AverageNum.length;
	}
	public static int Summation(double Number, int[] array){
		double sum = 0;
		for(int x = 0; x < array.length-1; x++){
			sum+= Number;
		}
		return sum;
	}
}
